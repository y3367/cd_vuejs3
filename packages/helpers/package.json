{
  "name": "@cd-vuejs3/helpers",
  "version": "0.2.3",
  "description": "vuejs3 helpers",
  "keywords": [
    "cd_vuejs3"
  ],
  "author": "y3367",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/y3367/cd_vuejs3.git"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./unstyled": {
      "types": "./dist/unstyled.d.ts",
      "import": "./dist/unstyled.js",
      "require": "./dist/unstyled.cjs"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "scripts": {
    "dev": "tsc --watch",
    "prebuild": "rimraf ./dist",
    "build": "npm run prebuild && npm run build:all",
    "build:all": "npm run build:cjs && npm run build:esm",
    "build:cjs": "rimraf ./dist/cjs && tsc --project tsconfig.cjs.json",
    "build:esm": "rimraf ./dist/esm && tsc --project tsconfig.esm.json",
    "test": "jest",
    "test0": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@cd-vuejs3/utils": "workspace: *",
    "@vueuse/components": "^10.5.0",
    "@vueuse/core": "^10.5.0",
    "axios": "^1.6.0",
    "bowser": "^2.11.0",
    "dayjs": "^1.11.10",
    "decimal.js": "^10.4.3",
    "element-plus": "^2.4.2",
    "is-unicode-supported": "^2.0.0",
    "js-base64": "^3.7.5",
    "lodash-es": "^4.17.21",
    "mobile-detect": "^1.4.5",
    "pinia": "^2.1.7",
    "vue": "^3.3.8",
    "vue-request": "2.0.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@rushstack/eslint-patch": "^1.5.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.5",
    "@types/lodash-es": "^4.17.11",
    "@types/node": "^20.9.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.2",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-icons": "^0.17.3",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.5.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  }
}

{
  "name": "@cd-vuejs3/utils",
  "version": "0.2.3",
  "description": "vuejs3 utils",
  "keywords": [
    "cd_vuejs3",
    "utils"
  ],
  "author": "y3367",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/y3367/cd_vuejs3.git"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./unstyled": {
      "types": "./dist/unstyled.d.ts",
      "import": "./dist/unstyled.js",
      "require": "./dist/unstyled.cjs"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "scripts": {
    "dev": "tsc --watch",
    "prebuild": "rimraf ./dist",
    "build": "npm run prebuild && npm run build:all",
    "build:all": "npm run build:cjs && npm run build:esm",
    "build:cjs": "rimraf ./dist/cjs && tsc --project tsconfig.cjs.json",
    "build:esm": "rimraf ./dist/esm && tsc --project tsconfig.esm.json",
    "test": "jest"
  },
  "dependencies": {
    "bowser": "^2.11.0",
    "dayjs": "^1.11.10",
    "decimal.js": "^10.4.3",
    "mobile-detect": "^1.4.5"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.8",
    "chalk": "^5.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1"
  }
}

{
  "name": "@cd-vuejs3/utils",
  "version": "0.0.4",
  "description": "vuejs3 utils",
  "keywords": [
    "cd_vuejs3"
  ],
  "author": "y3367",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/y3367/cd_vuejs3.git"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./unstyled": {
      "types": "./dist/unstyled.d.ts",
      "import": "./dist/unstyled.js",
      "require": "./dist/unstyled.cjs"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "npm run prebuild && npm run build-only",
    "build-only": "tsc",
    "prebuild": "rm -rf dist",
    "test-no": "echo \"Error: no test specified\" && exit 1",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "dependencies": {
    "bowser": "^2.11.0",
    "dayjs": "^1.11.9",
    "decimal.js": "^10.4.3",
    "mobile-detect": "^1.4.5"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@types/jest": "^29.5.3",
    "chalk": "^5.3.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "ts-jest": "^29.1.1"
  }
}
